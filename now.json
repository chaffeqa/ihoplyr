{
  "version": 2,
  "name": "ihoplyr",
  "routes": [	
    {	
      "src": "^/service-worker.js$",	
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "s-maxage=60",
        "Service-Worker-Allowed": "/"
      }
    }	
  ],
  "builds": [
  	{
      "src": "package.json",
      "use": "@now/static-build"
    }
  ]
}
